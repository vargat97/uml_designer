<Window x:Class="DiagramDesigner.View.AddBasicView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DiagramDesigner.View"
        mc:Ignorable="d"
        Title="AddBasicView" Height="200" Width="300"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <TextBox x:Name="NameTextbox" HorizontalAlignment="Left" Height="23" Margin="64,79,0,0" TextWrapping="Wrap" Text="{Binding Path=NodeName}" VerticalAlignment="Top" Width="120"/>
        <TextBlock HorizontalAlignment="Left" Margin="10,79,0,0" TextWrapping="Wrap" Text="Name:" VerticalAlignment="Top"/>
        <Button Content="Add" HorizontalAlignment="Left" Margin="207,139,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>

    </Grid>
</Window>
